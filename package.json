{
  "name": "@evatirum/utils",
  "version": "0.0.0-development",
  "description": "JavaScript Utilities",
  "keywords": [],
  "homepage": "https://github.com/evatrium/utils#readme",
  "repository": {
    "type": "git",
    "url": "https://github.com/evatrium/utils.git"
  },
  "bugs": {
    "url": "https://github.com/evatrium/utils/issues"
  },
  "license": "MIT",
  "publishConfig": {
    "access": "public"
  },
  "main": "dist/utils.cjs",
  "module": "dist/utils.mjs",
  "types": "dist/types/index.d.ts",
  "source": "src",
  "exports": {
    ".": {
      "import": "./dist/utils.mjs",
      "require": "./dist/utils.cjs"
    }
  },
  "files": [
    "dist"
  ],
  "scripts": {
    "dev": "vite",
    "build": "tsc && vite build",
    "preview": "vite preview",
    "_test": "jest --runInBand",
    "_test:coverage": "jest --runInBand --coverage",
    "test": "vitest",
    "test:coverage": "vitest run --coverage",
    "test:ci": "vitest --run",
    "lint": "eslint \"src/**/*.{ts,tsx}\"",
    "lint:fix": "npm run lint --fix",
    "lint:types": "tsc --noEmit",
		"commit": "git-cz",
    "semantic-release": "semantic-release"
  },
  "lint-staged": {
    "*.{js,ts,tsx}": [
      "npm run lint:fix"
    ]
  },
  "devDependencies": {
    "@testing-library/dom": "^8.19.0",
    "@testing-library/jest-dom": "^5.16.5",
    "@testing-library/user-event": "^14.4.3",
    "@types/jsdom": "^20.0.0",
    "@types/node": "17.0.21",
    "@typescript-eslint/eslint-plugin": "^5.40.0",
    "@typescript-eslint/parser": "^5.40.0",
    "commitizen": "4.2.5",
    "cz-conventional-changelog": "3.3.0",
    "eslint": "8.22.0",
    "eslint-config-prettier": "^8.5.0",
    "eslint-plugin-prettier": "^4.2.1",
    "husky": "^8.0.0",
    "jsdom": "^20.0.0",
    "lint-staged": "^13.0.3",
    "plop": "^3.1.1",
    "prettier": "^2.7.1",
    "semantic-release": "^19.0.5",
    "ts-node": "^10.9.1",
    "typescript": "4.6.2",
    "vite": "^3.1.6",
    "vite-plugin-dts": "0.9.10",
    "vite-tsconfig-paths": "3.4.1",
    "vitest": "^0.24.0"
  },
  "config": {
    "commitizen": {
      "path": "./node_modules/cz-conventional-changelog"
    }
  }
}
